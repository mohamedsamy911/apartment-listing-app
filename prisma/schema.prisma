generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.1.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Apartment {
  id          String @id @default(uuid())
  name        String
  unitNumber  String
  project     String
  description String?
  price       Float
  imageUrls   String[] @default([])
  createdAt   DateTime @default(now())
  contactNumber String
}